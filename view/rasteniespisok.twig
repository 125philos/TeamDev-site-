<!-- ***************** --><!-- КАРТОЧКИ РАСТЕНИЙ --><!-- ***************** -->{% extends "base.twig" %}{% block rows %}		<div class="row">			<!-- Навигация по видам растений -->		<div class="col-sm-3 sidenav">					<button type="button" class="btn btn-primary">Добавить растение</button>			</br></br>					<p class="lead">Виды растений</p>			<div class="list-group">				{% for cgenre in genre %}					<a href="#" class="list-group-item" id="cgenre-{{ cgenre.id_genre }}">{{ cgenre.genre_name }}</a>				{% endfor %}			</div>		</div>				<!-- Карточки растений -->		<div class="col-sm-9 text-left">					<h5>РАСТЕНИЯ ФЕРМЫ</h5>					<!-- Ряды карточек -->			<div class="row recipes-container">				{% for key, cperformer in performer %}					<div class="col-xs-3">						<div class="thumbnail">							<!-- Фото растения -->							<img src="{{ cperformer.performer_photo }}" alt="">							<div class="caption">								<!-- Сорт растения(+ переход по ссылке на страницу растения) -->								<h4><a href="/rasteniespisok/{{ cperformer.id_performer }}">Сорт растения</a></h4>									<p>Сорт растения</p>									<p>окраска плодов</p>									<p>Дата высадки</p>							</div>						</div>					</div>				{% endfor %}			</div>		</div>			</div>{% endblock %}