<!doctype html><html lang="en">  <head>    <!-- Required meta tags -->    <meta charset="utf-8">    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">    <!-- Bootstrap CSS -->    <link href="../css/bootstrap.min.css" rel="stylesheet">    <link href="../css/style.css" rel="stylesheet">    <title>SoundCloud</title>  </head>    <body>		<!-- Панель навигации -->    <nav class="navbar navbar-inverse">        <div class="container">		    <div class="navbar-header">                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">                    <span class="icon-bar"></span>                    <span class="icon-bar"></span>                    <span class="icon-bar"></span>                </button>            </div>            <div class="collapse navbar-collapse" id="myNavbar">                <ul class="nav navbar-nav">                    <li class="active"><a href="index.htm">Главная</a></li>					<li><a href="/employee/">Добавить исполнителя</a></li>                </ul>                <ul class="nav navbar-nav navbar-right">                    <li><a href="#"><span class="glyphicon glyphicon-log-in"></span> Войти</a></li>                </ul>            </div>        </div>    </nav>    <div class="container">	{% block rows %}	        <div class="row">					<!-- Навигация по жанрам -->		    <div class="col-sm-3 sidenav">                <p class="lead">Музыкальные жанры</p>                <div class="list-group">					{% for cgenre in genre %}						<a href="#" class="list-group-item" id="cgenre-{{ cgenre.id_genre }}">{{ cgenre.genre_name }}</a>					{% endfor %}                </div>            </div>					<!-- Карточки исполнителей -->            <div class="col-sm-9 text-left">				<!-- Ряды карточек -->                <div class="row recipes-container">					{% for key, cperformer in performer %}						<div class="col-xs-5">							<div class="thumbnail">								<img src="{{ cperformer.performer_photo }}" alt="">								<div class="caption">									<h4><a href="/employee/{{ cperformer.id_performer }}"> {{ cperformer.performer_nickname }} </a></h4>									<h5>{{ cperformer.performer_name }}</h5>									</h4>																		<!-- Поиск страны исполнителя -->									{% for ccountry in country %}										{% if ccountry.id_country == cperformer.id_country %}											<p>{{ ccountry.counry_name }}</p>										{% endif %}									{% endfor %}														</div>							</div>						</div>					{% endfor %}                </div>            </div>			        </div>	{% endblock %}    </div>		<!-- Подвал -->	<footer class="container-fluid text-center">        <p>© SoundCloud 2018</p>    </footer>	    <!-- Optional JavaScript -->    <!-- jQuery first, then Popper.js, then Bootstrap JS -->    <script src="js/jquery-3.3.1.slim.min.js"></script>    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>    <script src="js/bootstrap.min.js"></script>	  </body></html>